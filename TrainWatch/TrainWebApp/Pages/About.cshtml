@page
@model TrainWebApp.Pages.AboutModel
@{
}
@if (Model.CurrentBuildVersion == null)
{
    <div class="alert alert-warning" role="alert">
        Build Version Unknown
    </div>
}
else
{
    <div class="alert alert-info" role="alert">
        Database Version: @Model.CurrentBuildVersion.Major . @Model.CurrentBuildVersion.Minor . @Model.CurrentBuildVersion.Build
        &nbsp;
        released on @Model.CurrentBuildVersion.ReleaseDate.ToString("MMM dd, yyyy")
    </div>
}